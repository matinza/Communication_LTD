{
    "password": {
        "length": 10,
        "chars": {
            "uppercase": true,
            "lowercase": true,
            "numbers": true,
            "special": true
        },
        "history_limit": 3,
        "avoid_sequence": [
            "abc",
            "123"
        ]
    },
    "login": {
        "num_of_login_attempts": 3
    }
}